item(FEAT_TRAINS, item_d443, 1103) {
    property {
        track_type                   : RAIL;
        engine_class                 : ENGINE_CLASS_DIESEL;
        name                         : string(STR_TRAIN_DIESEL_D443_NAME);
        speed                        : 130 km/h;
        power                        : 1250 hp;
        weight                       : 61 ton;
        tractive_effort_coefficient  : 0.3;
        climates_available           : bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL);
        introduction_date            : date(1967, 1, 1);
        model_life                   : VEHICLE_NEVER_EXPIRES;
        vehicle_life                 : 50;
        cost_factor                  : ENGINE_DIESEL_COST_L;
        running_cost_base            : RUNNING_COST_DIESEL;
        running_cost_factor          : ENGINE_DIESEL_UPKEEP_L;
        cargo_capacity               : 0;
        refittable_cargo_classes     : bitmask(NO_CARGO_CLASS);
        non_refittable_cargo_classes : bitmask();
        dual_headed                  : 0;
        sprite_id                    : SPRITE_ID_NEW_TRAIN;
        visual_effect_and_powered    : visual_effect_and_powered(VISUAL_EFFECT_DIESEL, 0, DISABLE_WAGON_POWER);
        misc_flags                   : bitmask(TRAIN_FLAG_2CC);
